# .env
# Application Configuration
VITE_APP_TITLE=KrushiKranti Admin Panel

# API Configuration (Default to Local Spring Boot Port)
# Spring Boot default is 8080. We use /api/v1 as a standard prefix.
VITE_API_URL=http://localhost:8080/api/v1

# Feature Flags (Optional - good for controlling unreleased modules)
VITE_ENABLE_LOGISTICS_MODULE=true